        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.21",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "vue": "^2.6.12",
        "vue-loader": "^15.10.1",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.12.0",
        "icheck-bootstrap": "^3.0.1",
        "overlayscrollbars": "^1.10.2"
    }
